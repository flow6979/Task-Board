{# templates/admin/add.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}
<h1 class="text-primary">Add Admin</h1>

{{ form_start(form) }}
    {{ form_row(form.email) }}
    {{ form_row(form.password) }}
    {{ form_row(form.fullName) }}
    {{ form_row(form.phoneNumber) }}
    
    <button class="btn btn-primary">Add Admin</button>
{{ form_end(form) }}
{% endblock %}
